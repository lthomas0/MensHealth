{
  "name": "menshealth",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.4.0",
    "@material-ui/icons": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^3.6.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "create-react-app": "^1.5.2",
    "dotenv": "^6.0.0",
    "email-validator": "^2.0.4",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "express.js": "^1.0.0",
    "flash": "^1.1.0",
    "helmet": "^3.13.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.3.10",
    "morgan": "^1.9.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "password-validator": "^4.1.1",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.5",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-form": "^7.4.2",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.0.3",
    "winston": "^3.0.0"
  },
  "scripts": {
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles/importer.scss ./src/styles/importer.css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "NODE_ENV=production REACT_APP_ENV_URL=http://menshealthno.com node ./src/server/server.js",
    "start-dev-server": "REACT_APP_ENV_URL=http://localhost:8080 nodemon ./server/server.js",
    "start-dev-client": "npm-run-all -p watch-css start-js",
    "build-css": "node-sass-chokidar ./src/styles/importer.scss ./src/styles/importer.css",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "eject": "react-scripts eject",
    "test": "mocha --inspect-brk",
    "dev": "NODE_ENV=development REACT_APP_ENV_URL=http://localhost:8080  concurrently --kill-others-on-fail \"npm run start-dev-server\" \"npm run start-dev-client\"",
    "prod": "run-s build start-dev-server"
  },
  "description": "MensHealth Project - 2018",
  "main": "server.js",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raymundoxmartinez/MensHealth"
  },
